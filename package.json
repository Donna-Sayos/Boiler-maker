{
  "name": "boiler-maker",
  "version": "1.0.0",
  "description": "boilermaker workshop",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev": "webpack -w & nodemon server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "set JWT=secret_key nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test set JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" -compilers js:babel-register",
    "test:dev": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Donna-Sayos/Boiler-maker.git"
  },
