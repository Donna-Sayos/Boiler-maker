    "chai": "^4.3.6",
    "css-loader": "^6.7.1",
    "enzyme": "^3.11.0",
    "history": "^5.3.0",
    "mocha": "^10.0.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
